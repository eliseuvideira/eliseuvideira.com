{
  "compilerOptions": {
    "target": "es5",
    "module": "es6",
    "jsx": "react",
    "sourceMap": true,
    "outDir": "./build",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true
  },
  "exclude": ["node_modules", "public", ".cache"]
}
